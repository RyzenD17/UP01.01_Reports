<Window x:Class="UP01._01.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP01._01"
        mc:Ignorable="d"
        Title="Склад" Height="800" Width="900" MinHeight="800" MinWidth="900" Icon="icon.ico">
    
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="205*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition ></RowDefinition>
            <RowDefinition Height="14*"></RowDefinition>
        </Grid.RowDefinitions>

        <StackPanel Grid.Row="0" Grid.Column="0" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Image Source="Resources/Черновик.png" Width="35" Height="50"></Image>
            <TextBlock FontSize="30" Margin="10,0,0,0" Width="100">Склад</TextBlock>
        </StackPanel>

        <Frame Name="MainFrame" Grid.Column="0" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
