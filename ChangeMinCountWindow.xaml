<Window x:Class="UP01._01.ChangeMinCountWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UP01._01"
        mc:Ignorable="d"
        Title="ChangeMinCountWindow" Height="150" Width="300" MinHeight="150" MinWidth="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBox Width="150" Height="30" Name="TBCount" Margin="0,10,0,0"></TextBox>
            <Button Width="150" Height="30" Margin="0,5,0,0" Click="Button_Click">Сохранить</Button>
        </StackPanel>
    </Grid>
</Window>
